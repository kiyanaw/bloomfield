<w canon="ispî" class="ipc" gcat="IPC" sgloss="when" stem="ispî">ispîh</w>
<w canon="manihkomânêk" class="vai" gcat="2pAIimp" sgloss="take_knife" stem="manihkomânê" suffix="-k">manihkomânêk</w>
!</q>
<w canon="itwêwak" class="vai" gcat="3pAI" sgloss="say" stem="itwê" suffix="-wak">itwêwak</w>
<w canon="nêhiyawak" class="na" gcat="3pNA" sgloss="Cree" stem="nêhiyaw" suffix="-ak">nêhiyawak</w>
.
