</seg>

<seg n="16.3">
<w canon="pêyak" class="ipc" gcat="IPC" sgloss="one" stem="pêyak">pêyak</w>
<w canon="câpês" class="na" gcat="3sNA" sgloss="Capes" stem="câpês" suffix="0">câpês</w>
<w canon="isiyîhkâsôw" class="vai" gcat="3sAI" sgloss="be_called" stem="isiyîhkâso" suffix="-w">siyihkâsôw</w>
;
